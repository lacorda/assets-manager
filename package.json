{
  "name": "@lacorda/assets-manager",
  "version": "0.1.6",
  "description": "Webpack 插件：扫描项目本地图片资源并生成报告",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist --clean",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "webpack": ">=5"
  },
  "keywords": [
    "webpack",
    "plugin",
    "assets",
    "images",
    "report"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
